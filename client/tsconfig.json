{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": false,
    "target": "es6",
    "useUnknownInCatchVariables": false
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "next.d.ts",
    "next-env.d.ts"
, "src/sections/dashboard/overview/overview-done-tasks.js", "src/pages/login.js", "src/pages/index.js", "src/layouts/dashboard/index.js", "src/layouts/dashboard/index.js", "src/layouts/dashboard/config.js", "src/layouts/dashboard/layout.js", "src/layouts/dashboard/layout.js"  ],
  "exclude": [
    "node_modules"
  ]
}
